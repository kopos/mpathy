<h1>New beamer</h1>

<%= error_messages_for :beamer %>

<% form_for(@beamer) do |f| %>
  <p>
    <b>Location</b><br />
    <%= f.text_field :location %>
  </p>

  <p>
    <b>Alias</b><br />
    <%= f.text_field :alias %>
  </p>

  <p>
    <b>Auth code</b><br />
    <%= f.text_field :auth_code %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', beamers_path %>
